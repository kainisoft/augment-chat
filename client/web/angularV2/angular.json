        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/shared-lib/**/*.ts",
              "projects/shared-lib/**/*.html"
            ],
            "eslintConfig": "projects/shared-lib/eslint.config.js"
          }
        }
      }
    },
