{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/bootstrap": [
        "libs/bootstrap/src"
      ],
      "@app/bootstrap/*": [
        "libs/bootstrap/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/database": [
        "libs/database/src"
      ],
      "@app/database/*": [
        "libs/database/src/*"
      ],
      "@app/domain": [
        "libs/domain/src"
      ],
      "@app/domain/*": [
        "libs/domain/src/*"
      ],
      "@app/dtos": [
        "libs/dtos/src"
      ],
      "@app/dtos/*": [
        "libs/dtos/src/*"
      ],
      "@app/events": [
        "libs/events/src"
      ],
      "@app/events/*": [
        "libs/events/src/*"
      ],
      "@app/graphql": [
        "libs/graphql/src"
      ],
      "@app/graphql/*": [
        "libs/graphql/src/*"
      ],
      "@app/iam": [
        "libs/iam/src"
      ],
      "@app/iam/*": [
        "libs/iam/src/*"
      ],
      "@app/kafka": [
        "libs/kafka/src"
      ],
      "@app/kafka/*": [
        "libs/kafka/src/*"
      ],
      "@app/logging": [
        "libs/logging/src"
      ],
      "@app/logging/*": [
        "libs/logging/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/security": [
        "libs/security/src"
      ],
      "@app/security/*": [
        "libs/security/src/*"
      ],
      "@app/testing": [
        "libs/testing/src"
      ],
      "@app/testing/*": [
        "libs/testing/src/*"
      ],
      "@app/validation": [
        "libs/validation/src"
      ],
      "@app/validation/*": [
        "libs/validation/src/*"
      ]
    }
  }
}